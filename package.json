{
  "name": "my_plants_api",
  "version": "1.0.0",
  "description": "My Plants server app",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest --config test/jest-unit.config.ts",
    "pretest:int": "docker-compose -f docker-compose.test.yml up -d",
    "test:int": "dotenv -e .env.test -- npx prisma migrate reset -f && dotenv -e .env.test -- jest -i --no-cache --config test/jest-int.config.ts",
    "posttest:int": "docker-compose -f docker-compose.test.yml down"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schedule": "^2.1.0",
    "@prisma/client": "^3.15.2",
    "aws-sdk": "^2.1181.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.4",
    "dotenv-cli": "^6.0.0",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "nest-winston": "^1.8.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "sharp": "^0.30.7",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.1",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/swagger": "^6.1.0",
    "@nestjs/testing": "^9.0.11",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/nodemailer": "^6.4.5",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "prisma": "^3.15.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "16.17.0"
  }
}
